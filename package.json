{
  "name": "rsbuild-react-ts",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "check": "biome check --write",
    "dev:frontend": "rsbuild dev --open --environment web",
    "dev:backend": "bun --hot src/server.ts",
    "dev": "concurrently 'bun run dev:frontend' 'bun run dev:backend' -n frontend,backend",
    "format": "biome format --write",
    "generate-types": "kysely-codegen --exclude-pattern _* --out-file src/db/types.ts --camel-case --date-parser string",
    "preview": "rsbuild preview"
  },
