{
  "name": "logistics-management-go",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "generate:types": "bunx pocketbase-typegen --db ./pb_data/data.db --out web/lib/pocketbase-types.ts",
    "frontend": "rsbuild dev",
    "backend": "go run cmd/main.go",
    "publish": "bun run build && bun run push && bun run deploy",
    "push": "docker image push registry.marahuyo.dev/logistics-management-system:latest",
    "deploy": "docker context use marahuyo-dev && docker compose -f .docker/compose.prod.yaml pull && docker compose -f .docker/compose.prod.yaml up -d && docker context use default",
    "build": "docker context use default && docker build --build-arg PUBLIC_DOMAIN_NAME=f-org-e.systems -t registry.marahuyo.dev/logistics-management-system:latest -f .docker/Dockerfile ."
  },
  "devDependencies": {
    "@faker-js/faker": "^9.6.0",
    "@rsbuild/core": "^1.2.19",
    "@rsbuild/plugin-babel": "^1.0.4",
    "@rsbuild/plugin-solid": "^1.0.5",
    "@types/bun": "latest",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@ark-ui/solid": "^5.1.1",
    "@corvu/drawer": "^0.2.3",
    "@corvu/otp-field": "^0.1.4",
    "@corvu/resizable": "^0.2.4",
    "@fontsource/inter": "^5.2.5",
    "@kobalte/core": "^0.13.9",
    "@solid-primitives/refs": "^1.1.0",
    "@solidjs/router": "^0.15.3",
    "chart.js": "^4.4.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk-solid": "^1.1.2",
    "embla-carousel-solid": "^8.5.2",
    "lucide-solid": "^0.483.0",
    "pocketbase": "^0.25.2",
    "solid-js": "^1.9.5",
    "solid-sonner": "^0.2.8",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7"
  }
}
