{
  "name": "@f-orge/logistics-management-go",
  "private": true,
  "type": "module",
  "version": "0.1.0",
  "scripts": {
    "build": "bun run build:frontend && bun run build:backend",
    "build:frontend": "rsbuild build",
    "build:backend": "go build -o .output/server main.go",
    "check": "biome check --write",
    "dev": "concurrently \"bun run dev:backend\" \"bun run dev:frontend\" -n backend,frontend",
    "dev:frontend": "rsbuild dev --open",
    "dev:backend": "go run main.go serve",
    "format": "biome format --write",
    "preview": "./.output/server serve",
    "generate:pb-types": "bunx pocketbase-typegen --db ./pb_data/data.db -o lib/pocketbase.gen.ts",
    "local:ci": "bun run check && bun run format"
  },
  "dependencies": {
    "@marahuyo/react-ui": "git+ssh://github.com/Marahuyo-Developers/ui.git#main",
    "@tanstack/react-query": "^5.75.7",
    "@tanstack/react-router": "^1.117.1",
    "pocketbase": "^0.26.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rsbuild/core": "^1.3.1",
    "@rsbuild/plugin-react": "^1.1.1",
    "@tailwindcss/postcss": "^4.1.4",
    "@tanstack/router-plugin": "^1.117.2",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "concurrently": "^9.1.2",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.2"
  }
}