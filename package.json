  "packageManager": "bun@1.2.0",
  "type": "module",
  "scripts": {
    "dev": "bun concurrently 'bun --filter @apps/backend dev' 'bun --filter @apps/frontend dev' -n 'backend,frontend'",
    "build": "bun run build:packages && bun run build:apps",
    "build:packages": "bun run --filter '@packages/*' build",
    "build:apps": "bun run --filter '@apps/*' build",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "lint:all": "bun run --filter '*' lint",
    "format:all": "bun run --filter '*' format",
    "typecheck": "bun run --filter '*' typecheck"
  },
