{
  "compilerOptions": {
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "noEmit": true,
    "composite": true,
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "paths": {
      "@/*": ["./src/*"],
    }
  },
  "include": ["src/**/*","src/routeTree.gen.ts"],
  "exclude": ["node_modules", "dist"],
}
