{
  "name": "@packages/rpc",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*"
    }
  },
  "scripts": {
    "build": "tsc --build && tsc-alias -p tsc-alias.config.json",
    "dev": "tsc --build --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src"
  },
  "dependencies": {
    "@orpc/client": "^1.9.3",
    "@orpc/contract": "^1.9.4",
    "@orpc/server": "^1.9.3",
    "@packages/db": "workspace:*",
    "@tanstack/react-query": "^5.90.5",
    "hono": "^4.9.12",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
